{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "./src",
    "paths": {
      "@Src/*": ["/*"],
      "@Components/*": ["/components/*"],
      "@Pages/*": ["/pages/*"],
      "@Libs/*": ["/libs/*"],
      "@Redux/*": ["/redux/*"],
      "@Queries/*": ["/queries/*"],
      "@Router/*": ["/router/*"],
      "@Screens/*": ["/screens/*"],
      "@Utils/*": ["/utils/*"],
      "@Assets/*": ["/assets/*"],
      "@Example/*": ["/example/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
